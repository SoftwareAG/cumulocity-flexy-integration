{
    "apiVersion": "1",
    "version": "0.1.1-SNAPSHOT",
    "provider": {
        "name": "Cumulocity"
    },
    "isolation": "PER_TENANT",
    "requiredRoles": [
        "ROLE_INVENTORY_READ",
        "ROLE_INVENTORY_CREATE",
        "ROLE_INVENTORY_ADMIN",
        "ROLE_IDENTITY_READ",
        "ROLE_IDENTITY_ADMIN",
        "ROLE_DEVICE_CONTROL_READ",
        "ROLE_DEVICE_CONTROL_ADMIN"],
    "roles": [],
    "livenessProbe":{
		"httpGet":{
			"path": "/health",
			"port": 80
		},
		"initialDelaySeconds": 70,
		"periodSeconds": 30
	}
}